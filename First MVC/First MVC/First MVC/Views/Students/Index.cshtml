<!-- Views/Students/Index.cshtml -->
@model List<Student>

<h2>Student List</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.id</td>
                <td>@student.name</td>
                <td>@student.age</td>
            </tr>
        }
    </tbody>
</table>

